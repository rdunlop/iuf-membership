<!DOCTYPE html>
<html>
  <head>
    <title>IUF Membership management</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="https://www.paypal.com/sdk/js?client-id=<%= Rails.configuration.paypal_client_id %>&currency=<%= Rails.configuration.currency %>">
</script>
  </head>

  <body>
    <%= render "layouts/nav_bar" %>
    <%= render "layouts/development_banner" %>
    <%= render "layouts/flashes" %>


    <main role="main" class="container">
      <%= yield %>
    </main>
    <%= render "layouts/footer" %>
  </body>
</html>
